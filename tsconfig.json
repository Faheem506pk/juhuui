{
	"compilerOptions": {
		"target": "es2017",
		"module": "es2015",
		"moduleResolution": "node",
		"lib": ["es2019", "dom"],
		"experimentalDecorators": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"jsx": "react",
		"jsxFactory": "h",
		"baseUrl": ".",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true
	}
}
